<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=gbk" />
		#CSSM(["easyui","style"])
		#JSM(["jquery", "easyui"])
		#JS_ENV_VAL()
	</head>
	<body>
		<form method="post" target="hideFrame">
			<input type="hidden" id="eventId" name="eventId" value="$!{eventId}" >
			<input type="hidden" id="ids" name="ids">
			<div class="easyui-panel" title="查询条件" data-options="collapsible:true,iconCls:'icon-search'">
				<table width="100%" border="0" cellpadding="0" cellspacing="1">
					<tr>
						<td class="head" noWrap>查阅人</td>
						<td><input type="text" id="userName" name="userName"/></td>
						<td>
							<a href="javascript:search()" class="easyui-linkbutton" iconCls="icon-search">Search</a>
							<a href="javascript:clearValue()" class="easyui-linkbutton" iconCls="icon-clear">Clear</a>
						</td>
					</tr>
				</table>
			</div>
			<table id="datagrid">  
			</table>
		</form>
		<iframe id="hideFrame" name="hideFrame"  style="height: 0; width: 0"></iframe>
		<div id="authorizeAddMain" />
	</body>
	<script type="text/javascript" src="$!{env.imgUrl}/js/platform/wfe/authorizeEvent/searchEventReader.js"></script>
</html>