package com.kintiger.platform.news.pojo;

import com.kintiger.platform.base.pojo.SearchInfo;
/**
 * Title: 系统更新和bug修改栏目是否阅读
 * Description: basisPlatform
 * @author: xg.chen
 * @date:2016年12月14日 下午4:56:13
 */
public class IsRead extends SearchInfo {
	
	private static final long serialVersionUID = -1703188498168522675L;
	
	private Long id;
	private Long delailId;//明细表id
	private Long totalId;//总表id
	private String userId;//登陆的用户ID
	private String isRead;//是否阅读标识
	
	
	public Long getId() {
		return id;
	}
	public void setId(Long id) {
		this.id = id;
	}
	public Long getDelailId() {
		return delailId;
	}
	public void setDelailId(Long delailId) {
		this.delailId = delailId;
	}
	public Long getTotalId() {
		return totalId;
	}
	public void setTotalId(Long totalId) {
		this.totalId = totalId;
	}
	public String getUserId() {
		return userId;
	}
	public void setUserId(String userId) {
		this.userId = userId;
	}
	public String getIsRead() {
		return isRead;
	}
	public void setIsRead(String isRead) {
		this.isRead = isRead;
	}
	
}
