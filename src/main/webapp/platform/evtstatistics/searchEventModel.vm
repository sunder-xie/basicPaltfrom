<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=gbk" />
		#CSSM(["easyui","style"])
		#JSM(["jquery", "easyui"])
		#JS_ENV_VAL()
	</head>
	<body>
		<form method="post">
			<div id="dd" class="easyui-panel" title="查询条件" data-options="collapsible:true,iconCls:'icon-search'" style="padding:5px">
				<table width="100%" border="0" cellpadding="0" cellspacing="1">
					<tr>
						<td class="head" noWrap>
							模板名称:
						</td>
						<td>
							<input type="text" class="easyui-validatebox" id="modelName" name="modelName" style="width:115px" />
						</td>
						<td class="head" noWrap>
							事务类型:
						</td>
						<td>
							<select id="modelType" name="modelType" class="easyui-combobox" data-options="editable:false">
							    <option value="">请选择...</option>
							    <option value="1">协同办公</option>
							    <option value="2">客户关系管理</option>
							    <option value="3">营销系统管理</option>
							    <option value="4">人事档案管理</option>
							    <option value="5">经销商管理</option>
							</select>
						</td>
						<td>
							<a href="javascript:search()" class="easyui-linkbutton" iconCls="icon-search">查询</a>
						</td>
                    </tr>
				</table>
			</div>
			<table id="datagrid">  
			</table> 
		</form>
		<iframe id="hideFrame" name="hideFrame"  style="height: 0; width: 0"></iframe>
		<div id="maintWindow"/>
	</body>
	<script type="text/javascript" src="$!{env.imgUrl}/js/platform/evtstatistics/searchEventModel.js"></script>
</html>